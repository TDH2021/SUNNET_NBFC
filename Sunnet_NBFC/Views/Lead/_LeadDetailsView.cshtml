@model Sunnet_NBFC.Models.clsLeadGenerationMaster

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<div class="modal-dialog" style="max-width: 1000px;">

    <!-- Modal content-->
    <div class="modal-content" style="overflow-y: scroll; height:550px; width: 1050px;">
        <div class="modal-header">
            <h4 class="modal-title">Customer Details</h4>

        </div>
        <div class="modal-body">
            <div class="panel-body">
                <table class="table table-striped table-bordered table-hover table-responsive" style="display:table;"
                       id="dataTables-example-Details">
                    <thead>
                        <tr>
                            <th>Lead NO</th>
                            <td>@Model.LeadNo</td>
                            <th>Customer Name</th>
                            <td>@Model.FirstName  @Model.MiddleName  @Model.LastName</td>


                        </tr>
                        <tr>
                            <th>Main Product</th>
                            <td>@Model.MainProductName</td>
                            <th>Sub Product</th>
                            <td>@Model.ProductName</td>
                        </tr>



                        <tr>
                            <th>Father Name</th>
                            <td>@Model.FatherName</td>
                            <th>Mother Name</th>
                            <td>@Model.MotherName</td>
                        </tr>
                        <tr>
                            <th>Spouse Name</th>
                            <td>@Model.SpouseName</td>
                            <th>Gender</th>
                            <td>@Model.Gender</td>
                        </tr>
                        <tr>
                            <th>Date Of Birth</th>
                            <td>@Model.DateofBirth</td>

                            <th>Martial Status</th>
                            <td>@Model.MartialStatus</td>
                        </tr>
                        <tr>
                            <th>Present Address</th>
                            <td>@Model.PresentAddress</td>
                            <th>Present Pincode</th>
                            <td>@Model.PresentPincode</td>

                        </tr>
                        <tr>
                            <th>Present State</th>
                            <td>@Model.PresentStateName</td>
                            <th>Present City</th>
                            <td>@Model.PresentCityName</td>

                        </tr>


                        <tr>
                            <th>Permanent Address</th>
                            <td>@Model.PermanentAddress</td>
                            <th>Permanent Pincode</th>
                            <td>@Model.PermanentPincode</td>

                        </tr>
                        <tr>
                            <th>Permanent State</th>
                            <td>@Model.PremenentStateName</td>
                            <th>Permanent City</th>
                            <td>@Model.PremenentCityName</td>

                        </tr>

                        <tr>
                            <th>Cibil Score</th>
                            <td>@Model.CibilScore</td>
                            <th>Mobile No1</th>
                            <td>@Model.MobileNo1</td>
                        </tr>
                        <tr>
                            <th>Father Mobile No</th>
                            <td>@Model.FatherMobileNo</td>
                            <th>Mother Mobile No</th>
                            <td>@Model.MotherMobileNo</td>
                        </tr>


                        <tr>
                            <th>Spouse Mobile No</th>
                            <td>@Model.SpouseMobileNo</td>
                            <th>Aadhar No</th>
                            <td>@Model.AadharNo</td>
                        </tr>

                        <tr>
                            <th>Pan No</th>
                            <td>@Model.PanNo</td>
                            <th>Email Id</th>
                            <td>@Model.EmailId</td>
                        </tr>


                        <tr>
                            <th>Refernce Name</th>
                            <td>@Model.RefernceName</td>

                            <th>Refence Relation</th>
                            <td>@Model.RefenceRelation</td>

                        </tr>

                        <tr>
                            <th>Refence MobileNo</th>
                            <td>@Model.RefenceMobileNo</td>

                            <th>No of Dependent</th>
                            <td>@Model.NoofDependent</td>

                        </tr>



                        @if (ViewBag.CUSTYPEREUIRED == "c" || ViewBag.CUSTYPEREUIRED == "b")
                        {
                            <tr>
                                <th><b>Co Applicant Details:</b></th>
                                <td colspan="4"></td>
                            </tr>

                            <tr>

                                <th>CO Applicant Name</th>
                                <td>@Model.CO_FirstName  @Model.CO_MiddleName  @Model.CO_LastName</td>


                            </tr>

                            <tr>

                                <th>Gender</th>
                                <td>@Model.CO_Gender</td>
                                <th>Date Of Birth</th>
                                <td>@Model.CO_DOB</td>

                            </tr>
                            <tr>

                                <th>Martial Status</th>
                                <td>@Model.CO_Marital_Status</td>
                                <th>Present Address</th>
                                <td>@Model.CO_PresentAddress</td>
                            </tr>
                            <tr>

                                <th>Present Pincode</th>
                                <td>@Model.CO_PresentPinCode</td>

                                <th>Present State</th>
                                <td>@Model.CO_PresentStateName</td>
                            </tr>
                            <tr>

                                <th>Present City</th>
                                <td>@Model.CO_PresentCityName</td>

                                <th>Permanent Address</th>
                                <td>@Model.CO_PermanentAddress</td>
                            </tr>

                            <tr>

                                <th>Permanent Pincode</th>
                                <td>@Model.CO_PermanentPincode</td>

                                <th>Permanent State</th>
                                <td>@Model.CO_PermanentStateName</td>
                            </tr>

                            <tr>

                                <th>Permanent City</th>
                                <td>@Model.CO_PermanentCityName</td>

                                <th>Mobile No</th>
                                <td>@Model.CO_Mobile_No</td>
                            </tr>

                            <tr>

                                <th>Aadhar No</th>
                                <td>@Model.CO_Adhaar</td>

                                <th>Pan No</th>
                                <td>@Model.CO_PAN</td>
                            </tr>

                            <tr>
                                <th>Email</th>
                                <td>@Model.CO_Email_Id</td>
                                <th></th>
                                <td></td>
                            </tr>





                        }












                    </thead>
                </table>


                @if (ViewBag.CUSTYPEREUIRED == "g" || ViewBag.CUSTYPEREUIRED == "b")
                {
                    <div>
                        <h4 class="modal-title"><b>Gurantor Details</b></h4>
                        <table class="table table-striped table-bordered table-hover table-responsive" id="example10">
                            <thead>
                                <tr>
                                    <th>Gurantor Name</th>
                                    <th>Gender</th>
                                    <th>Dob</th>
                                    <th>MartialStatus</th>
                                    <th>Current Address</th>
                                    <th>Pincode</th>
                                    <th>State</th>
                                    <th>City</th>
                                    <th>Permanent Address</th>
                                    <th>Pincode</th>
                                    <th>State</th>
                                    <th>City</th>
                                    <th>Mobile No</th>
                                    <th>Email No</th>
                                    <th>Pan No</th>
                                    <th>Aadhar No</th>
                                    <th>Cibil Score</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in ViewBag.gurantorslist)
                                {
                                    <tr>
                                        <td>@item.G_FirstName @item.G_MiddleName @item.G_LastName</td>

                                        <td>@item.G_Gender</td>
                                        <td>@item.G_DOB</td>
                                        <td>@item.G_Marital_Status</td>
                                        <td>@item.G_PresentAddress</td>
                                        <td>@item.G_PresentPinCode</td>
                                        <td>@item.G_PresentStateName</td>
                                        <td>@item.G_PresentCityName</td>
                                        <td>@item.G_PermanentAddress</td>
                                        <td>@item.G_PermanentPincode</td>
                                        <td>@item.G_PermanentStateName</td>
                                        <td>@item.G_PermanentCityName</td>
                                        <td>@item.G_Mobile_No</td>
                                        <td>@item.G_EmailId</td>
                                        <td>@item.G_PanNo</td>
                                        <td>@item.G_AadharNo</td>
                                        <td>@item.G_CibilScore</td>

                                    </tr>
                                }


                            </tbody>

                        </table>

                    </div>
                }


                <div>
                    <h4 class="modal-title"><b>Loan Details</b></h4>
                <div>

                    <table class="table table-striped table-bordered table-hover table-responsive" style="display:table;">
                        <thead>
                            <tr>

                                <th>Requested Loan Amount</th>
                                <td>@Model.ReuestedLoanAmount</td>

                                <th>Reuested Loan Tenure</th>
                                <td>@Model.ReuestedLoanTenure</td>
                            </tr>

                            <tr>

                                <th>Est Month Income</th>
                                <td>@Model.EstMonthIncome</td>

                                <th>Est Month Expense</th>
                                <td>@Model.EstMonthExpense</td>
                            </tr>

                            <tr>


                                <th>Colletral Security Type</th>
                                <td>@Model.ColletralSecurityType</td>
                                <th>Cur Month Obligation</th>
                                <td>@Model.CurMonthObligation</td>

                            </tr>

                            <tr>

                                
                                <th>FORecomed Amt</th>
                                <td>@Model.FORecomedAmt</td>
                            </tr>

                        </thead>
                    </table>


                </div>




                <div class="panel-body">
                    <label class="col-sm-4 control-label">Approved Loan Amount<span style="color:red">*</span></label>
                    <div class="col-sm-4">
                        <input type="number" id="rmark" name="rmark" autocomplete="off" class="form-control " />
                    </div>
                    <div class="col-sm-4">
                    </div>
                </div>

                <div class="panel-body">
                    <label class="col-sm-4 control-label">Approved Loan Tenure<span style="color:red">*</span></label>
                    <div class="col-sm-4">
                        <input type="number" id="rmark" name="rmark" autocomplete="off" class="form-control " />
                    </div>
                    <div class="col-sm-4">
                    </div>
                </div>
                <div class="panel-body">
                    <label class="col-sm-4 control-label">Status<span style="color:red">*</span></label>
                    <div class="col-sm-4">
                        <select class="form-control" id="VERIFICATION_TYPES" name="VERIFICATION_TYPES" autocomplete="off">
                            <option value="0">Select Action</option>
                            <option value="1">Approve</option>
                            <option value="2">Send Back</option>
                            <option value="3">Reject</option>

                        </select>
                    </div>
                    <div class="col-sm-4">
                    </div>
                </div>

                <div class="panel-body">
                    <label class="col-sm-4 control-label">Comment/Remarks<span style="color:red">*</span></label>
                    <div class="col-sm-4">
                        <textarea id="rmark" name="rmark" autocomplete="off" class="form-control "></textarea>
                    </div>
                    <div class="col-sm-4">
                    </div>
                </div>




                <div style="margin-bottom:10px;">
                    <div class="col-sm-4"></div>
                    <div class="col-md-6" style="margin-bottom:10%">
                        <button type="submit" class="btn btn-success" onclick="return Validation();">
                            Submit
                            &nbsp; &nbsp; <i class="fa fa-save">
                            </i>
                        </button>
                        &nbsp; &nbsp; &nbsp;
                        <button class="btn btn-danger text-right" data-dismiss="modal" aria-hidden="true" onclick="reloadfun();">
                            Close
                            &nbsp; &nbsp; <i class="fa fa-close"> </i>
                        </button>
                    </div>
                </div>
            </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-danger" onclick="reloadfun();">Close</button>
        </div>
    </div>

</div>


<script type="text/javascript">

    function reloadfun() {
        debugger
        location.reload();
    } 
</script>
